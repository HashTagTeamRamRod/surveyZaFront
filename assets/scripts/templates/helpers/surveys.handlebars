{{#each surveys as |survey|}}
  <p data-id="{{survey.id}}">
    <h3>{{survey.title}}</h3>
</p>
  <form>
    {{#each survey.questions as |question|}}
      {{> questions question=question}}
    {{/each}}
    <button data-id="{{survey.id}}" type="submit" class="vote">Vote</button>
    <input data-id="{{survey.id}}" type="reset" class="reset">
    <button data-id="{{survey.id}}" class="edits-survey" value="edit">Edit</button>
    <button data-id="{{survey.id}}" class="delete-survey" value="delete">Delete</button>
  </form>

{{/each}}
